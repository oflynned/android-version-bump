{
  "name": "android-semantic-release",
  "version": "0.0.0",
  "private": false,
  "description": "GitHub action for bumping Android repos using semantic releases.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oflynned/Android-Semantic-Release.git"
  },
  "keywords": [
    "android",
    "semantic",
    "version",
    "release"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "actions-toolkit": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.6",
    "jest-mock-extended": "^1.0.14",
    "jest-circus": "^27.0.6",
    "js-yaml": "^4.1.0",
    "npm-check-updates": "^11.8.3",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.2.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
